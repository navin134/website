			function boolThing(boolRadio) {
			var list;
			if(boolRadio){
				list = terms;
				
			}else{
				list = defs;
				
			}
			document.getElementById("image").style.visibility = "hidden";
			return list;
			
		}
		
		function displayCue() {
			var list = boolThing(false);
			alert(list);
			var counterThing = 0;
			do {
			    rand = parseInt(Math.random()*list.length);
			    boolList = [];
			    for(int i=0; i<check.length; i++) {
			        if(check[i]) {
			            boolList.push(i);
			            
			        }
			    }
				if(randChecker(rand, boolList) || (counterQ < list.length)){
					break;
					
				}
			} while(boolList.length > 0);
				document.getElementById("clue").value = list[rand];
				check[rand] = false;
				counterQ++;
			
		}
		 
		 function randChecker(rand, boolList) {
		     var rander = false;
		     for(int i=0; i<boolList.length; i++) {
		         if(rand == boolList[i]) {
		             rander = true;
		             break;
		         }
		     }
		     
		     return rander;
		 }
		 
		function checkAnswer() {
			var boolbool = true;
			var list = boolThing(boolbool);
			var answ = document.getElementById("answer").value;
			if(asnw == list[rand]) {
				document.getElementById("winLoss").src = "images/suc.jpeg";
			}else {
				document.getElementById("winLoss").src = "images/R.png";
			}
			var midPoint = window.innerWidth;
			document.getElementById("image").style.left = (midPoint/2)-158;
			document.getElementById("image").style.visibility = "visible";
			
		}
		
