		"kardashev scale",
		"a = g(m1*m2/r*r)",
		"1.02214076*10^23",
		"1/137",
		"james bohr",
		"demons"
		];
		var check = [
		true,
		true,
		true,
		true,
		true,
		true];
		//var check = [true, false, false, false, false, false];
		var counterQ = 0;
		var score = 0;
		var bool = false;
		var rand;
		
		
